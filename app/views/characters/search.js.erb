$("#characters").html("<%= escape_javascript(render(@characters)) %>");
characterSearch.bindEvents();
<% if @characters.last_page? %>
  $('.pagination').html("That's all, folks!");
<% else %>
  $('.pagination')
    .html("<%=j link_to_next_page(@characters, 'Next Page', params: { remote: true, q: '' }) %>");
<% end %>
